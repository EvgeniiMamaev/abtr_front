{
  "compilerOptions": {
    "outDir": "./public/js/",
    "lib": ["ES2020", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "module": "ES2020",
    "target": "ES2020",
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
